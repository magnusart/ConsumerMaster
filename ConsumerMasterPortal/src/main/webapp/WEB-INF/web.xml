<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name>ConsumerMasterPortal</display-name>
  
  <servlet>
    <servlet-name>consumerServiceServlet</servlet-name>
    <servlet-class>com.magnusart.crm.portal.server.ConsumerServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>consumerServiceServlet</servlet-name>
    <url-pattern>/portal/consumerService</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>